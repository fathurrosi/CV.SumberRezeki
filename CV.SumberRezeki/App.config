<?xml version="1.0"?>
<configuration>
	<configSections>
		<section name="Custom.Config" type="Custom.Config.CustomSection, Custom.Config"/>
	</configSections>
	<Custom.Config>
		<Default applicationName="CV.SumberRezekiInternasional" connectionStringName="ConnString"/>
		<Entities>
			<add assembly="DBHelper" connectionStringName="ConnString"/>
		</Entities>
	</Custom.Config>
	<appSettings>
		<clear/>

		<add key="ThousandSeparator" value="." />
		<add key="CommaSeparator" value="," />
		<add key="TopMargin" value="0"/>
		<add key="PageSize" value="25"/>
		<add key="CatalogUnit" value="Kg,Piece,Ekor,Col,Karung"/>

		<!--
    X adalah jarak horisontal dari batas kertas paling kiri
    diisi dengan angka, jika tidak akan diisi dengan angka nol
    -->
		<add key="TemplateFromat" value="0"/>
		<add key="X_No" value="7"/>
		<add key="X_Item" value="35"/>

		<add key="X_Qty" value="440"/>
		<!--kelipatan 120-->
		<add key="X_Colly" value="560"/>
		<!--kelipatan 120-->
		<add key="X_Harga" value="680"/>
		<!--kelipatan 120-->
		<add key="X_TotalHarga" value="800"/>
		<!--kelipatan 120-->

	</appSettings>
	<connectionStrings>
		<clear/>
		<add name="ConnString" connectionString="server=localhost;uid=root;pwd=;database=dbsrinternasional_v2;Allow User Variables=True" providerName="MySql.Data.MySqlClient"/>
	</connectionStrings>
	<startup>

		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
	</startup>
</configuration>
